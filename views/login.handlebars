<div class="forms">
<div class="loginclass">
<form class="login-form">
  
  <div>
  <h2> Login In</h2>
    <label for="email-login">Email:</label>
    <input type="text" id="email-login" />
  </div>
  <div>
    <label for="password-login">Password:</label>
    <input type="password" id="password-login" />
  </div>
  <div>
    <button type="submit">Login</button>
  </div>
</form>
</div>

<div class="signupclass">
<form class="signup-form">
  <h2>Sign Up</h2>
  <div>
        
    <label for="username-signup">Username:</label>
    <input type="text" id="username-signup" />
  </div>
  <div>
    <label for="email-signup">Email:</label>
    <input type="text" id="email-signup" />
  </div>
  <div>
    <label for="password-signup">Password:</label>
    <input type="password" id="password-signup" />
  </div>
  <div>
    <label for="license-signup">Lender License #:</label>
    <input type="license" id="license-signup" />
  </div>
  <div>
    <label for="mortgage-signup">Mortgage Company:</label>
    <input type="mortgage" id="mortgage-signup" />
  </div>
  <div>
    <label for="phone-signup">Phone Number:</label>
    <input type="phone" id="phone-signup" />
  </div>
  <div>
    <button type="submit">signup</button>
  </div>
</form>
</div>
</div>
<script src="/javascript/login.js"></script>